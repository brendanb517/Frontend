    />\n          <input\n            type=\"password\"\n            placeholder=\"Password\"\n            name=\"password\"\n            onChange={handleChange}\n            required\n          />\n          <button>Sign Up</button>\n          <span>\n            Have an account?\n            <Link to=\"/login\" className=\"Link\">\n              Sign In\n            </Link>\n          </span>\n        </form>\n      </div>\n    </div>\n  );\n};\n\nexport default Register;\n","/home/brendanb517/Development/code/Phase-3/Ruby/Frontend/src/pages/Login/Login.jsx",[],[],"/home/brendanb517/Development/code/Phase-3/Ruby/Frontend/src/pages/Home/Home.jsx",[],[],"/home/brendanb517/Development/code/Phase-3/Ruby/Frontend/src/components/Navbar/navbar.jsx",["79"],[],"import \"./navbar.scss\";\nimport defaultUser from \"../../assets/default-user.png\";\nimport { Link } from \"react-router-dom\";\n\nimport NotificationsNoneOutlinedIcon from \"@mui/icons-material/NotificationsNoneOutlined\";\nimport PersonOutlineOutlinedIcon from \"@mui/icons-material/PersonOutlineOutlined\";\nimport SearchOutlinedIcon from \"@mui/icons-material/SearchOutlined\";\n\nconst Navbar = () => {\n  return (\n    <div className=\"navbar\">\n      <div className=\"left\">\n        <Link to=\"/\" style={{ textDecoration: \"none\" }}>\n          <span>FlatSocial</span>\n        </Link>\n        {/* <div className=\"search\">\n          <SearchOutlinedIcon />\n          <input type=\"text\" placeholder=\"Search\" />\n        </div> */}\n      </div>\n      <div className=\"right\">\n        <PersonOutlineOutlinedIcon />\n        <NotificationsNoneOutlinedIcon />\n        <div className=\"user\">\n          <img src={defaultUser} alt=\"\" />\n          <span>Dwayne</span>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Navbar;\n","/home/brendanb517/Development/code/Phase-3/Ruby/Frontend/src/components/rightbar/rightBar.jsx",[],[],"/home/brendanb517/Development/code/Phase-3/Ruby/Frontend/sr